@InProceedings{Timashev2020,
  author                     = {Timashev, S. A.},
  booktitle                  = {{INTERNATIONAL CONFERENCE SAFETY PROBLEMS OF CIVIL ENGINEERING CRITICAL INFRASTRUCTURES}},
  title                      = {{Black-Swan Type Catastrophes and Antifragility/Supra-resilience of Urban Socio-Technical Infrastructures}},
  doi                        = {{10.1088/1757-899X/972/1/012001}},
  language                   = {{English}},
  location                   = {{DIRAC HOUSE, TEMPLE BACK, BRISTOL BS1 6BE, ENGLAND}},
  note                       = {{5th International Conference Safety Problems of Civil Engineering Critical Infrastructures (SPCECI), Ural Fed Univ, Ekaterinburg, RUSSIA, MAY 21-22, 2019}},
  organization               = {{Russian Acad Sci, Ural Branch; Russian Acad Sci, Ural Branch, Sci \& Engn Ctr Reliabil \& Safety Large Syst \& Machines; Russian Acad Architecture \& Construct Sci, Ural Territorial Branch; S Ural State Univ; European Civil Engn Educ \& Training}},
  publisher                  = {{IOP PUBLISHING LTD}},
  series                     = {{IOP Conference Series-Materials Science and Engineering}},
  volume                     = {{972}},
  abstract                   = {{This paper may be one of the first attempts dealing with the problem of
   creating, providing and maintaining antifragility of systems of
   interdependent urban critical infrastructures (CI) in the wake of
   black-swan type technological, ecological, economic or social
   catastrophes occurring in a municipality. A synonym is offered to
   describe antifragility from a positive psychology perspective,
   formulating the problem as the supraresilience problem. A brief
   description is given of the developed innovative approach for creating a
   supraresilient city/region using black-swan catastrophe and the
   antifragility concepts. Resilience metrics are formulated as well as
   methods of assessing damage, interdependence of infrastructures and
   convergent technologies and sciences needed for practical regional
   resilience and risk management of the system of systems (SoS) of
   interdependent urban critical infrastructures).}},
